{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/inst377-final/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/inst377-final/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/inst377-final/dist/$1"
    }
  ]
}


